.reverseOrder {
  background: url("../images/icons/positiveOrder.svg") 100% 100% no-repeat;
}

.positiveOrder {
  background: url("../images/icons/reverseOrder.svg") 100% 100% no-repeat;
}

.layout_content {
  -webkit-overflow-scrolling: touch;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 2.5rem;
  overflow-y: scroll;
}

.module {
  background: #fff;
  margin-bottom: .5rem;
}

.warp {
  margin-top: 2.5rem;
  /*padding-top: 2.5rem;*/
}

.center {
  text-align: center;
}

.textR {
  text-align: right;
}

.textL {
  text-align: left;
}

.bl {
  border-left: .07rem solid #ddd;
}

.br {
  border-right: .07rem solid #ddd;
}

.bt {
  border-bottom: .07rem solid #ddd;
}

.mt {
  margin-bottom: .5rem;
}

.m0 {
  margin: 0;
}

.relative {
  position: relative;
}

.van-toast--default {
  width: 4rem;
  height: 4rem;
  min-height: 4rem;
}

.header {
  height: 2.5rem;
  line-height: 2.5rem;
  background: #1a7fe9;
}

.header .van-nav-bar__title {
  color: #fff;
  font-size: 1.1rem;
}

.header .van-nav-bar__text {
  color: #fff;
  font-size: 1rem;
}

.header .van-nav-bar__right .van-nav-bar__text {
  font-size: 0.875rem;
}

.header .van-nav-bar__text:active {
  background: #1a7fe9;
}

.van-nav-bar .van-icon {
  color: #fff;
}

.header {
  z-index: 2000 !important;
}

.a {
  color: #0066cc;
}

.gray {
  color: #969799;
}

.grey {
  color: #8d8d8d;
}

.red {
  color: #e44444;
}

.green {
  color: #4ebc7f;
}

.blank {
  color: #000;
}

.bgWhite {
  background: #fff;
}

.successMsg {
  color: #4ebc7f;
}

.bottomBtn {
  bottom: 0;
  top: auto;
  width: 100%;
  height: 2.5rem;
  padding: 0;
  position: fixed;
  /*background-color: #197fe8;*/
  margin-left: auto;
  margin-right: auto;
}

.p9 {
  padding: 0 .9rem;
}

.p5 {
  padding: 0 .5rem;
}

.p2 {
  padding: 0 .2rem;
}

.lh2 {
  line-height: 2rem;
}

.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}

.tip-empty {
  padding-top: 50%;
  text-align: center;
}

.icon-emptyface {
  height: 2rem;
  line-height: 2rem;
  display: inline-block;
  background-repeat: no-repeat;
  background-image: url("../images/icons/emptyface.svg");
  padding-left: 2.5rem;
  color: #d3d3d3;
}

.text-empty {
  color: gray;
  line-height: 2rem;
  vertical-align: text-bottom;
}
